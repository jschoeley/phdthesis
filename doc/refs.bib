@Article{Smith2001,
  author   = {Smith, Gordon C. S.},
  journal  = {American Journal of Obstetrics and Gynecology},
  title    = {Life-table analysis of the risk of perinatal death at term and post term in singleton pregnancies},
  year     = {2001},
  number   = {3},
  pages    = {489--496},
  volume   = {184},
  abstract = {OBJECTIVE This study was undertaken to estimate the cumulative risk of perinatal death associated with delivery at each gestational week both at term and post term. STUDY DESIGN The numbers of antepartum stillbirths, intrapartum stillbirths, neonatal deaths, and surviving neonates delivered at between 37 and 43 weeks' gestation in Scotland, 1985-1996, were obtained from national databases (n = 700,878) after exclusion of multiple pregnancies and deaths caused by congenital abnormality. The numbers of deaths at each gestational week were related to appropriate denominators: antepartum stillbirths were related to ongoing pregnancies, intrapartum stillbirths were related to all births (excluding antepartum stillbirths), and neonatal deaths were related to live births. The cumulative probability of perinatal death associated with delivery at each gestational week was estimated by means of life-table analysis. RESULTS The gestational week of delivery associated with the lowest cumulative risk of perinatal death was 38 weeks' gestation, whereas the perinatal mortality rate was lowest at 41 weeks' gestation. The risk of death increased more sharply among primigravid women after 38 weeks' gestation because of a greater risk of antepartum stillbirth. The relationships between risk of death and gestational age were similar for the periods 1985-1990 and 1991-1996. CONCLUSION Delivery at 38 weeks' gestation was associated with the lowest risk of perinatal death.},
  doi      = {10.1067/mob.2001.109735},
  file     = {:Smith2001 - Life Table Analysis of the Risk of Perinatal Death at Term and Post Term in Singleton Pregnancies.pdf:PDF},
  groups   = {gestational_age, the_thesis},
  pmid     = {11228508},
}

